    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "cwd": "${workspaceFolder}",
            "executable": "./build/pow.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "external",
            "device": "STM32U585AI",
            // "ipAddress": "2.55.114.141",
            "gdbTarget": "localhost:50000",
            "svdFile": "framework/BSP/MCU/VendorSpecific/ST/st-hal/devices/STM32U5/SVD/STM32U5xx.svd"
        },
        {
            "name": "Attach",
            "cwd": "${workspaceFolder}",
            "executable": "./build/pow.elf",
            "request": "attach",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "external",
            "device": "STM32U585AI",
            // "ipAddress": "2.55.114.141",
            "gdbTarget": "localhost:50000",
            "svdFile": "framework/BSP/MCU/VendorSpecific/ST/st-hal/devices/STM32U5/SVD/STM32U5xx.svd"
        }
    ],
}